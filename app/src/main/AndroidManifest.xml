        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:icon">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ThemNhanVienActivity"
            android:label="Thêm Nhân Viên" />
        <activity
            android:name=".DanhSachNhanVienActivity"
            android:label="Danh Sách Nhân Viên" />
        <activity
            android:name=".ThietLapTruongPhoPhongActivity"
            android:label="Chọn Trưởng/Phó Phòng" />
        <activity
            android:name=".SuaNhanVienActivity"
            android:label="Sửa Nhân Viên" />
        <activity android:name=".ChuyenPhongBanActivity"
            android:label="Chuyen Phong Ban"/>

    </application>

</manifest>